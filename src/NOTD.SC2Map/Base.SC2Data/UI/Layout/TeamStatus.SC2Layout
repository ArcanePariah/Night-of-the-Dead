<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
	<Constant name = "StatusOffset" val = "-10"/>
	<Constant name = "WHITE" val = "255,255,255" />
	<Frame type="Label" name="PlusLabelTemplate">
		<Style val="@@StandardTooltip"/>
		<ScaleText val= "True"/>
		<CollapseLayout val = "true" />
		<Text val = "" />
	</Frame>
	
	<Frame type="Image" name="AilementTemplate">
        <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
        <Height val="18"/>
        <Width val="18"/>
    </Frame>

    <Frame type="Image" name="AilementBackgroundTemplate">
        <Height val="22"/>
        <Width val="22"/>
        <Texture val="Assets\Textures\ui_research_frame_tooltip_protoss.dds"/>
        <TextureType val="HorizontalBorder"/>
        <BlendMode val="Add"/>
		<CollapseLayout val="true"/>
        <Frame type="Image" name="AilmentImage" template="TeamStatus/AilementTemplate">
        </Frame>
		<Visible val = "False" />
    </Frame>

    <Frame type="UnitStatusFrame" name="PetStats">
        <Height val="26"/>
        <Width val="172"/>

        <Frame type="UnitStatusHealthBar" name="PetStatusHealth">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="5"/>
            <Height val="26"/>
            <Width val="172"/>
            <BackgroundShown val="true"/>
            <BackgroundColor val="180,0,0"/>
            <BackgroundInsetRect top="0" left="0" bottom="0" right="0"/>
            <BarInsetRect top="0" left="0" bottom="0" right="0"/>
            <Texture val="@UI/HealthBar"/>
            <OwnerColor val="200,200,0" index="0"/>
            <AllyColor val="200,200,0" index="0"/>
            <ReductionShown val="false" index="0"/>
        </Frame>
		
		<Frame type="UnitStatusHealthLabel" name="HealthLabel">
			<ShowPercent val="true"/>
			
			<Frame type="Label" name="StatusLabel">
				<Anchor relative="$parent"/>
				<Style val="ModCenterSize16"/>
				<Visible val= "True"/>
			</Frame>
			<Anchor  relative="$parent/PetStatusHealth" offset="0"/>
		</Frame>
		
		<Frame type="Label" name="LeaverLabel">
			<Anchor  relative="$parent/PetStatusHealth" offset="0"/>
            <Style val="ModCenterSize16"/>
        </Frame>

    </Frame>
	
	<Frame type="UnitStatusFrame" name="UnitStats">
        <AcceptsMouse val="true"/>
        <Height val="26"/>
        <Width val="172"/>

        <Frame type="UnitStatusHealthBar" name="UnitStatusHealth">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Height val="26"/>
            <Width val="172"/>
            <BackgroundShown val="true"/>
            <BackgroundColor val="180,0,0"/>
            <BackgroundInsetRect top="0" left="0" bottom="0" right="0"/>
            <BarInsetRect top="0" left="0" bottom="0" right="0"/>
            <Texture val="@UI/HealthBar"/>
            <OwnerColor val="0,180,0" index="0"/>
            <AllyColor val="0,180,0" index="0"/>
            <ReductionShown val="false" index="0"/>
        </Frame>
		
		<Frame type="UnitStatusHealthLabel" name="HealthLabel">
			<ShowPercent val="true"/>
			
			<Frame type="Label" name="StatusLabel">
				<Visible val= "True"/>
				<Anchor relative="$parent"/>
				<Style val="ModCenterSize16"/>
			</Frame>
			<Anchor  relative="$parent/UnitStatusHealth" offset="0"/>
		</Frame>
		
		
		<Frame type = "Frame" name = "AilmentContainer">
			<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
			<Anchor side="Left" relative="$parent" pos="Min" offset="3"/>
			<Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
			<Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
			
			<Frame type="Image" name="Ailement01" template="TeamStatus/AilementBackgroundTemplate">
				<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
				<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
				<Color val = "255,128,0"/>
				<Frame type="Image" name="AilmentImage">
					<Texture val = "Assets\Textures\btn-tips-fallenheroes.dds"/>
				</Frame>
			</Frame>

			<Frame type="Image" name="Ailement02" template="TeamStatus/AilementBackgroundTemplate">
				<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
				<Anchor side="Left" relative="$parent/Ailement01" pos="Max" offset="0"/>
				<Color val = "255,0,0"/>
				<Frame type="Image" name="AilmentImage">
					<Texture val = "Assets\Textures\btn-upgrade-zerg-enduringcorruption.dds"/>
				</Frame>
			</Frame>

			<Frame type="Image" name="Ailement03" template="TeamStatus/AilementBackgroundTemplate">
				<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
				<Anchor side="Left" relative="$parent/Ailement02" pos="Max" offset="0"/>
				<Color val = "128,255,0"/>
				<Frame type="Image" name="AilmentImage">
					<Texture val = "Assets\Textures\btn-upgrade-zerg-airattacks-level0.dds"/>
				</Frame>
				<Tooltip val = "@Behavior/Tooltip/Venom"/>
			</Frame>

			<Frame type="Image" name="Ailement04" template="TeamStatus/AilementBackgroundTemplate">
				<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
				<Anchor side="Left" relative="$parent/Ailement03" pos="Max" offset="0"/>
				<Color val = "255,255,255"/>

				<Frame type="Image" name="AilmentImage">
					<Texture val = "Assets\Textures\btn-ability-zerg-leech.dds"/>
				</Frame>
			</Frame>
			
			<Frame type="Image" name="Ailement05" template="TeamStatus/AilementBackgroundTemplate">
				<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
				<Anchor side="Left" relative="$parent/Ailement04" pos="Max" offset="0"/>
				<Color val = "0,255,255"/>
				<Frame type="Image" name="AilmentImage">
					<Texture val = "Assets\Textures\btn-ability-terran-emp-color.dds"/>
				</Frame>
			</Frame>
			
			<Frame type="Image" name="Ailement06" template="TeamStatus/AilementBackgroundTemplate">
				<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
				<Anchor side="Left" relative="$parent/Ailement05" pos="Max" offset="0"/>
				<Color val = "255,0,255"/>
				<Frame type="Image" name="AilmentImage">
					<Texture val = "Assets\Textures\btn-ability-terran-tosh-mindblast.dds"/>
				</Frame>
			</Frame>
			
			<Frame type="Image" name="Ailement07" template="TeamStatus/AilementBackgroundTemplate">
				<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
				<Anchor side="Left" relative="$parent/Ailement06" pos="Max" offset="0"/>
				<Color val = "255,0,0"/>
				<Frame type="Image" name="AilmentImage">
					<Texture val = "Assets\Textures\btn-upgrade-zerg-airattacks-level3.dds"/>
				</Frame>
			</Frame>
			
		</Frame>
        
		<Frame type="UnitStatusBehaviorAnim" name="ShowCripple">
            <Anchor relative="$parent"/>
            <Behavior val="Cripple"/>
			<Animation name="Show">
				<Event event="BehaviorCreate" action="Reset, Play"/>
				<Event event="BehaviorDestroy" action="Stop"/>

                <Controller type="Visibility" end="Pause" frame="$parent/AilmentContainer/Ailement01">
                    <Key type="Bool" time="0" value="False"/>
                    <Key type="Bool" time="0" value="True"/>
                </Controller>
            </Animation>
        </Frame>
		
		<Frame type="UnitStatusBehaviorAnim" name="ShowOpenWound">
            <Anchor relative="$parent"/>
            <Behavior val="OpenWound"/>

            <Animation name="Show">
				<Event event="BehaviorCreate" action="Reset, Play"/>
				<Event event="BehaviorDestroy" action="Stop"/>

                <Controller type="Visibility" end="Pause" frame="$parent/AilmentContainer/Ailement02">
                    <Key type="Bool" time="0" value="False"/>
                    <Key type="Bool" time="0" value="True"/>
