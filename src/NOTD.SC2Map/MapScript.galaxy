//--------------------------------------------------------------------------------------------------
// Trigger: Ammo UI Button Callback
//--------------------------------------------------------------------------------------------------
bool gt_AmmoUIButtonCallback_Func (bool testConds, bool runActions) {
    // Conditions
    if (testConds) {
        if (!((EventDialogControlEventType() == c_triggerControlEventTypeClick))) {
            return false;
        }

        if (!(((EventDialogControl() == gv_ammoUIMoveButton[1]) || (EventDialogControl() == gv_ammoUIMoveButton[2]) || (EventDialogControl() == gv_ammoUIMoveButton[3]) || (EventDialogControl() == gv_ammoUIMoveButton[4]) || (EventDialogControl() == gv_ammoUIMoveButton[5]) || (EventDialogControl() == gv_ammoUIMoveButton[6]) || (EventDialogControl() == gv_ammoUIMoveButton[7]) || (EventDialogControl() == gv_ammoUIMoveButton[8])))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    if (gv_ammoBarPosition[EventPlayer()] >= 1) {
        gv_ammoBarPosition[EventPlayer()] = 0;
    }
    else {
        gv_ammoBarPosition[EventPlayer()] = gv_ammoBarPosition[EventPlayer()] + 1;
    }
    gf_AmmoUIMove(EventPlayer());
    gf_UpdateAmmoDialog(EventPlayer());
