    autoBA69838F_n = lv_sCVCount;
    for (autoBA69838F_i = 1; autoBA69838F_i <= autoBA69838F_n; autoBA69838F_i += 1) {
        libNtve_gf_UnitCreateFacingPoint(1, "SCV", 0, 13, lv_reinforcementDropPoint, UnitGetPosition(gv_boss));
