    }
    if ((lp_reload == ge_ReloadType_Automatic) && (gv_nightmare == true)) {
        lv_reloadTime *= 2.0;
    }

    lv_reloadTime = gf_CommandoReload(lv_reloadTime, lp_reloadingUnit);
    lv_reloadTime = (lv_reloadTime - (IntToFixed(gv_statsAgility3[lp_player]) * 0.08));
