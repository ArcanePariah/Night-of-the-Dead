    // Implementation
    if ((gf_StatsGetMaxModPoints(lp_player) < 0)) {
        return true;
    }
    else if ((gv_modTotalPointsPositive[lp_player] > (gv_playerLifeTimeCreditsAcquired[lp_player] / 10000))) {
        return true;
    }
    else if ((((gv_playerRank[lp_player] * 2) + gf_GetMedalStats(lp_player)) < gf_GetCurrentAllocatedStats(lp_player))) {
        return true;
    }
    else if ((gf_BOInsigniaBought(lp_player) == true) && (gv_playerLifeTimeCreditsAcquired[lp_player] < 10000)) {
        return true;
    }
    else if ((gv_combatActionRibbon[lp_player] > gv_playerGamesPlayed[lp_player])) {
        return true;
    }
    else if (((gv_lifeSavingMedal[lp_player] + gv_silverStar[lp_player]) > gv_combatActionRibbon[lp_player])) {
        return true;
    }
    else if ((gf_StatsGetMaxModPoints(lp_player) > 20)) {
        return true;
    }
    else if ((gv_superiorServiceMedal[lp_player] < (MinI(gv_survivalWin[lp_player], 20) + gv_survivalWinNM[lp_player]))) {
        return true;
    }
    else if (((gv_statsModProperties[1][lp_player] + gv_statsModProperties[2][lp_player] + gv_statsModProperties[3][lp_player] + gv_statsModProperties[4][lp_player] + gv_statsModProperties[5][lp_player] + gv_statsModProperties[6][lp_player] + gv_statsModProperties[7][lp_player]) > 35)) {
        return true;
    }
    else if ((gf_StatsGetMaxModPoints(lp_player) < gv_modTotalPointsNegative[lp_player])) {
        return true;
    }
    return false;
}

