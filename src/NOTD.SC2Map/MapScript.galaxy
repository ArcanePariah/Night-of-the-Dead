                if ((lv_statValues[lv_strEnAgPeIn] < 9) && (gv_statsTotalStats[lv_index] > 0)) {
                    gf_StatsAddStat(lv_index, lv_strEnAgPeIn, UserDataGetGameLink("StatData", UserDataInstance("StatData", lv_strEnAgPeIn), "Behavior", 1));
