    }

    // Actions
    if (!runActions) {
        return true;
    }

    if ((lv_attackingWeapon == "Shotgun")) {
        gv_marineRounds[lv_attackingPlayer] = MaxI((gv_marineRounds[lv_attackingPlayer] - 2), 0);
