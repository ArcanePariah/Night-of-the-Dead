bool gt_STBoss2AI_Func (bool testConds, bool runActions) {
    // Variable Declarations
    unit lv_harbingerUnit;
    unitgroup lv_harbingerConsiderationSet;

    // Variable Initialization
    lv_harbingerConsiderationSet = UnitGroupEmpty();

    // Actions
    if (!runActions) {
