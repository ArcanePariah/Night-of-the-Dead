    }
    else {
        if ((RandomInt(0, 3) == 1) && ((UnitHasBehavior(lv_bOAttacker, "SpectreCloaking") == true) || (UnitHasBehavior(lv_bOAttacker, "SpectreCloaking2") == true)) && (UnitGroupHasUnit(gv_blackOps, lv_bODefender) == true)) {
