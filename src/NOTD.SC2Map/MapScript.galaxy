        if (!(((gv_victoryButton1 == EventDialogControl()) || (gv_victoryButton2 == EventDialogControl()) || (gv_scrollEndGameUIButtons[0] == EventDialogControl()) || (gv_scrollEndGameUIButtons[1] == EventDialogControl())))) {
